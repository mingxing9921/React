.components-header {
  box-sizing: border-box;
  padding: 15px 25px;
  border-bottom: 1px solid #ccc;
  background: white;
}
.components-header .caption {
  font-size: 20px;
  text-indent: 20px;
}
.components-progress {
  margin-top: 20px;
  width: 100%;
  height: 3px;
  background: #ddd;
  cursor: pointer;
}
.components-progress .progress {
  width: 5%;
  height: 3px;
  background: #2f9842;
}
